#!python

Import('env', 'PREREQS')

# Additional includes
env.Append(CPPPATH = ['#/src/include'])

# Generated libraries will be installed under build/lib
env.Append(LIBPATH = ['#/build/lib'])

# Builders
SConscript('common/SConscript', exports=['env', 'PREREQS'])
SConscript('dtp/SConscript', exports=['env', 'PREREQS'])
SConscript('server/SConscript',  exports='env')
SConscript('vos/SConscript', exports=['env', 'PREREQS'])
SConscript('dmg/SConscript', exports='env')
SConscript('dsm/SConscript',  exports='env')
SConscript('dsr/SConscript',  exports='env')
SConscript('dct/SConscript',  exports='env')
