"""Build object tests"""

def scons():
    """Execute build"""
    Import('denv')
    Import('common_tgts')

    test_tgt = denv.SharedObject('place_obj.c')
    denv.Program('pl_map', test_tgt + common_tgts,
                 LIBS=['daos_common', 'gurt', 'cart', 'placement'])

if __name__ == "SCons.Script":
    scons()
