#!python

Import('env')

# dsms: DSM Server Module/Library
dsms = env.SharedLibrary('daos_m_srv',
			 ['dsms_module.c', 'dsm_rpc.c', 'dsms_storage.c',
			  'dsms_pool.c', 'dsms_container.c', 'dsms_target.c'])
env.Install('#/build/lib/', dsms)

# dsmc: DSM Client Module/Library
dsmc = env.SharedLibrary('daos_m', ['dsm_rpc.c', 'dsmc_pool.c'])
env.Install('#/build/lib/', dsmc)
